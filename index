</span><span><span class="hljs-keyword">from</span></span><span> kivy.uix.widget </span><span><span class="hljs-keyword">import</span></span><span> Widget
</span><span><span class="hljs-keyword">from</span></span><span> kivy.graphics </span><span><span class="hljs-keyword">import</span></span><span> Rectangle, Color
</span><span><span class="hljs-keyword">from</span></span><span> plyer </span><span><span class="hljs-keyword">import</span></span><span> gps
</span><span><span class="hljs-keyword">from</span></span><span> kivy.clock </span><span><span class="hljs-keyword">import</span></span><span> Clock

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">GameScreen</span></span><span>(</span><span><span class="hljs-title class_ inherited__">Widget</span></span><span>):
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, **kwargs</span></span><span>):
        </span><span><span class="hljs-built_in">super</span></span><span>().__init__(**kwargs)
        self.character_pos = [self.center_x, self.center_y]
        self.last_location = </span><span><span class="hljs-literal">None</span></span><span>

        </span><span><span class="hljs-keyword">with</span></span><span> self.canvas:
            Color(</span><span><span class="hljs-number">1</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>)
            self.char = Rectangle(pos=self.character_pos, size=(</span><span><span class="hljs-number">50</span></span><span>, </span><span><span class="hljs-number">50</span></span><span>))

        gps.configure(on_location=self.on_gps_location)
        gps.start(minTime=</span><span><span class="hljs-number">1000</span></span><span>, minDistance=</span><span><span class="hljs-number">1</span></span><span>)  </span><span><span class="hljs-comment"># updates every second</span></span><span>

        Clock.schedule_interval(self.update_character, </span><span><span class="hljs-number">1</span></span><span>/</span><span><span class="hljs-number">30</span></span><span>)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">on_gps_location</span></span><span>(</span><span><span class="hljs-params">self, **kwargs</span></span><span>):
        lat = </span><span><span class="hljs-built_in">float</span></span><span>(kwargs[</span><span><span class="hljs-string">'lat'</span></span><span>])
        lon = </span><span><span class="hljs-built_in">float</span></span><span>(kwargs[</span><span><span class="hljs-string">'lon'</span></span><span>])
        self.last_location = (lat, lon)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">update_character</span></span><span>(</span><span><span class="hljs-params">self, dt</span></span><span>):
        </span><span><span class="hljs-keyword">if</span></span><span> self.last_location:
            </span><span><span class="hljs-comment"># Convert GPS to screen position (you’ll want better scaling in real app)</span></span><span>
            x = self.last_location[</span><span><span class="hljs-number">1</span></span><span>] * </span><span><span class="hljs-number">100</span></span><span>  </span><span><span class="hljs-comment"># longitude</span></span><span>
            y = self.last_location[</span><span><span class="hljs-number">0</span></span><span>] * </span><span><span class="hljs-number">100</span></span><span>  </span><span><span class="hljs-comment"># latitude</span></span><span>
            self.char.pos = (x % self.width, y % self.height)  </span><span><span class="hljs-comment"># wrap on screen</span></span><span>

</span><span><span class="hljs-keyword">class</span></span><span> </span><span><span class="hljs-title class_">GPSApp</span></span><span>(</span><span><span class="hljs-title class_ inherited__">App</span></span><span>):
    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">build</span></span><span>(</span><span><span class="hljs-params">self</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> GameScreen()

</span><span><span class="hljs-keyword">if</span></span><span> __name__ == </span><span><span class="hljs-string">"__main__"</span></span><span>:
    GPSApp().run()
</span></span></code></div></div></pre>
