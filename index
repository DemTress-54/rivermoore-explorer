  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">style</span></span></span><span>&gt;<span class="language-css">
    </span></span><span><span class="hljs-selector-tag">html</span></span><span>, </span><span><span class="hljs-selector-tag">body</span></span><span> { </span><span><span class="hljs-attribute">margin</span></span><span>: </span><span><span class="hljs-number">0</span></span><span>; </span><span><span class="hljs-attribute">padding</span></span><span>: </span><span><span class="hljs-number">0</span></span><span>; </span><span><span class="hljs-attribute">overflow</span></span><span>: hidden; </span><span><span class="hljs-attribute">background</span></span><span>: </span><span><span class="hljs-number">#eee</span></span><span>; }
    </span><span><span class="hljs-selector-id">#map-container</span></span><span> {
      </span><span><span class="hljs-attribute">position</span></span><span>: relative;
      </span><span><span class="hljs-attribute">width</span></span><span>: </span><span><span class="hljs-number">100vw</span></span><span>; </span><span><span class="hljs-attribute">height</span></span><span>: </span><span><span class="hljs-number">100vh</span></span><span>;
      </span><span><span class="hljs-attribute">background-image</span></span><span>: </span><span><span class="hljs-built_in">url</span></span><span>(</span><span><span class="hljs-string">'https://i.imgur.com/r9vKjhh.jpg'</span></span><span>);
      </span><span><span class="hljs-attribute">background-size</span></span><span>: cover; </span><span><span class="hljs-attribute">background-position</span></span><span>: center;
    }
    </span><span><span class="hljs-selector-id">#character</span></span><span> {
      </span><span><span class="hljs-attribute">position</span></span><span>: absolute;
      </span><span><span class="hljs-attribute">width</span></span><span>: </span><span><span class="hljs-number">40px</span></span><span>; </span><span><span class="hljs-attribute">height</span></span><span>: </span><span><span class="hljs-number">40px</span></span><span>;
      </span><span><span class="hljs-attribute">background</span></span><span>: </span><span><span class="hljs-built_in">url</span></span><span>(</span><span><span class="hljs-string">'https://upload.wikimedia.org/wikipedia/commons/1/14/Location_dot_red.svg'</span></span><span>) no-repeat center;
      </span><span><span class="hljs-attribute">background-size</span></span><span>: contain;
      </span><span><span class="hljs-attribute">transform</span></span><span>: </span><span><span class="hljs-built_in">translate</span></span><span>(-</span><span><span class="hljs-number">50%</span></span><span>, -</span><span><span class="hljs-number">50%</span></span><span>);
    }
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">style</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">head</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;<span class="hljs-name">body</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"map-container"</span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"character"</span></span><span>&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">const</span></span><span> char = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'character'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> map = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'map-container'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> parkCenter = { </span><span><span class="hljs-attr">lat</span></span><span>: </span><span><span class="hljs-number">34.0425</span></span><span>, </span><span><span class="hljs-attr">lon</span></span><span>: -</span><span><span class="hljs-number">84.0965</span></span><span> };
    </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">SCALE</span></span><span> = </span><span><span class="hljs-number">100000</span></span><span>;

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">updatePosition</span></span><span>(</span><span><span class="hljs-params">pos</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> lat = pos.</span><span><span class="hljs-property">coords</span></span><span>.</span><span><span class="hljs-property">latitude</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> lon = pos.</span><span><span class="hljs-property">coords</span></span><span>.</span><span><span class="hljs-property">longitude</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> dx = (lon - parkCenter.</span><span><span class="hljs-property">lon</span></span><span>) * </span><span><span class="hljs-variable constant_">SCALE</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> dy = (parkCenter.</span><span><span class="hljs-property">lat</span></span><span> - lat) * </span><span><span class="hljs-variable constant_">SCALE</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> cx = map.</span><span><span class="hljs-property">clientWidth</span></span><span> / </span><span><span class="hljs-number">2</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> cy = map.</span><span><span class="hljs-property">clientHeight</span></span><span> / </span><span><span class="hljs-number">2</span></span><span>;
      char.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">left</span></span><span> = </span><span><span class="hljs-string">`<span class="hljs-subst">${cx + dx}</span></span></span><span>px`;
      char.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">top</span></span><span> = </span><span><span class="hljs-string">`<span class="hljs-subst">${cy + dy}</span></span></span><span>px`;
    }

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showError</span></span><span>(</span><span><span class="hljs-params">err</span></span><span>) { </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"GPS error: "</span></span><span> + err.</span><span><span class="hljs-property">message</span></span><span>); }

    </span><span><span class="hljs-keyword">if</span></span><span> (navigator.</span><span><span class="hljs-property">geolocation</span></span><span>) {
      navigator.</span><span><span class="hljs-property">geolocation</span></span><span>.</span><span><span class="hljs-title function_">watchPosition</span></span><span>(updatePosition, showError, {
        </span><span><span class="hljs-attr">enableHighAccuracy</span></span><span>: </span><span><span class="hljs-literal">true</span></span><span>,
        </span><span><span class="hljs-attr">maximumAge</span></span><span>: </span><span><span class="hljs-number">0</span></span><span>,
        </span><span><span class="hljs-attr">timeout</span></span><span>: </span><span><span class="hljs-number">10000</span></span><span>
      });
    } </span><span><span class="hljs-keyword">else</span></span><span> </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Geolocation not supported."</span></span><span>);
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">html</span></span></span><span>&gt;
</span></span></code></div></div></pre>
<hr data-start="1885" data-end="1888">
<h3 data-start="1890" data-end="1922">🌐 Step 2: Hosting It Easily</h3>
<h4 data-start="1924" data-end="1973">🔹 Option A: GitHub Pages (free &amp; shareable)</h4>
<ol data-start="1975" data-end="2288">
<li data-start="1975" data-end="2015">
<p data-start="1978" data-end="2015"><span class="relative -mx-px my-[-0.2rem] rounded px-px py-[0.2rem] transition-colors duration-100 ease-in-out">Create (or log into) your GitHub account.</span></p>
</li>
<li data-start="2016" data-end="2056">
<p data-start="2019" data-end="2056"><span class="relative -mx-px my-[-0.2rem] rounded px-px py-[0.2rem] transition-colors duration-100 ease-in-out">Create a <strong data-start="9" data-end="27">new repository</strong>, name it something like <code data-start="52" data-end="73">rivermoore-explorer</code>.</span></p>
</li>
<li data-start="2057" data-end="2097">
<p data-start="2060" data-end="2097"><span class="relative -mx-px my-[-0.2rem] rounded px-px py-[0.2rem] transition-colors duration-100 ease-in-out">Upload the HTML file (name it <code data-start="30" data-end="42">index.html</code>).</span></p>
</li>
<li data-start="2098" data-end="2138">
<p data-start="2101" data-end="2138"><span class="relative -mx-px my-[-0.2rem] rounded px-px py-[0.2rem] transition-colors duration-100 ease-in-out">Go to <strong data-start="6" data-end="26">Settings → Pages</strong> in the repo, set the source to the <code data-start="62" data-end="68">main</code> branch root.</span></p>
</li>
<li data-start="2139" data-end="2247">
<p data-start="2142" data-end="2179"><span class="relative -mx-px my-[-0.2rem] rounded px-px py-[0.2rem] transition-colors duration-100 ease-in-out">GitHub will give you a URL such as:</span></p>
