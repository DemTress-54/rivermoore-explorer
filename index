      </span><span><span class="hljs-attribute">position</span></span><span>: absolute;
      </span><span><span class="hljs-attribute">width</span></span><span>: </span><span><span class="hljs-number">40px</span></span><span>;
      </span><span><span class="hljs-attribute">height</span></span><span>: </span><span><span class="hljs-number">40px</span></span><span>;
      </span><span><span class="hljs-attribute">background</span></span><span>: </span><span><span class="hljs-built_in">url</span></span><span>(</span><span><span class="hljs-string">'https://upload.wikimedia.org/wikipedia/commons/thumb/1/14/Location_dot_red.svg/2048px-Location_dot_red.svg.png'</span></span><span>) no-repeat center;
      </span><span><span class="hljs-attribute">background-size</span></span><span>: contain;
      </span><span><span class="hljs-attribute">transform</span></span><span>: </span><span><span class="hljs-built_in">translate</span></span><span>(-</span><span><span class="hljs-number">50%</span></span><span>, -</span><span><span class="hljs-number">50%</span></span><span>);
    }
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">style</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">head</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;<span class="hljs-name">body</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"map-container"</span></span><span>&gt;
    </span><span><span class="hljs-tag">&lt;<span class="hljs-name">div</span></span></span><span> </span><span><span class="hljs-attr">id</span></span><span>=</span><span><span class="hljs-string">"character"</span></span><span>&gt;</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">div</span></span></span><span>&gt;

  </span><span><span class="hljs-tag">&lt;<span class="hljs-name">script</span></span></span><span>&gt;<span class="language-javascript">
    </span></span><span><span class="hljs-keyword">const</span></span><span> char = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'character'</span></span><span>);
    </span><span><span class="hljs-keyword">const</span></span><span> map = </span><span><span class="hljs-variable language_">document</span></span><span>.</span><span><span class="hljs-title function_">getElementById</span></span><span>(</span><span><span class="hljs-string">'map-container'</span></span><span>);

    </span><span><span class="hljs-comment">// Rivermoore Park approximate GPS center</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> parkCenter = {
      </span><span><span class="hljs-attr">lat</span></span><span>: </span><span><span class="hljs-number">34.0425</span></span><span>,
      </span><span><span class="hljs-attr">lon</span></span><span>: -</span><span><span class="hljs-number">84.0965</span></span><span>
    };

    </span><span><span class="hljs-comment">// How many pixels per GPS degree (rough approximation for 1 block zoom)</span></span><span>
    </span><span><span class="hljs-keyword">const</span></span><span> </span><span><span class="hljs-variable constant_">SCALE</span></span><span> = </span><span><span class="hljs-number">100000</span></span><span>;

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">updatePosition</span></span><span>(</span><span><span class="hljs-params">position</span></span><span>) {
      </span><span><span class="hljs-keyword">const</span></span><span> lat = position.</span><span><span class="hljs-property">coords</span></span><span>.</span><span><span class="hljs-property">latitude</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> lon = position.</span><span><span class="hljs-property">coords</span></span><span>.</span><span><span class="hljs-property">longitude</span></span><span>;

      </span><span><span class="hljs-keyword">const</span></span><span> dx = (lon - parkCenter.</span><span><span class="hljs-property">lon</span></span><span>) * </span><span><span class="hljs-variable constant_">SCALE</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> dy = (parkCenter.</span><span><span class="hljs-property">lat</span></span><span> - lat) * </span><span><span class="hljs-variable constant_">SCALE</span></span><span>;

      </span><span><span class="hljs-keyword">const</span></span><span> centerX = map.</span><span><span class="hljs-property">clientWidth</span></span><span> / </span><span><span class="hljs-number">2</span></span><span>;
      </span><span><span class="hljs-keyword">const</span></span><span> centerY = map.</span><span><span class="hljs-property">clientHeight</span></span><span> / </span><span><span class="hljs-number">2</span></span><span>;

      char.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">left</span></span><span> = </span><span><span class="hljs-string">`<span class="hljs-subst">${centerX + dx}</span></span></span><span>px`;
      char.</span><span><span class="hljs-property">style</span></span><span>.</span><span><span class="hljs-property">top</span></span><span> = </span><span><span class="hljs-string">`<span class="hljs-subst">${centerY + dy}</span></span></span><span>px`;
    }

    </span><span><span class="hljs-keyword">function</span></span><span> </span><span><span class="hljs-title function_">showError</span></span><span>(</span><span><span class="hljs-params">error</span></span><span>) {
      </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"GPS error: "</span></span><span> + error.</span><span><span class="hljs-property">message</span></span><span>);
    }

    </span><span><span class="hljs-keyword">if</span></span><span> (navigator.</span><span><span class="hljs-property">geolocation</span></span><span>) {
      navigator.</span><span><span class="hljs-property">geolocation</span></span><span>.</span><span><span class="hljs-title function_">watchPosition</span></span><span>(updatePosition, showError, {
        </span><span><span class="hljs-attr">enableHighAccuracy</span></span><span>: </span><span><span class="hljs-literal">true</span></span><span>,
        </span><span><span class="hljs-attr">maximumAge</span></span><span>: </span><span><span class="hljs-number">0</span></span><span>,
        </span><span><span class="hljs-attr">timeout</span></span><span>: </span><span><span class="hljs-number">10000</span></span><span>
      });
    } </span><span><span class="hljs-keyword">else</span></span><span> {
      </span><span><span class="hljs-title function_">alert</span></span><span>(</span><span><span class="hljs-string">"Geolocation is not supported by your browser."</span></span><span>);
    }
  </span><span><span class="hljs-tag">&lt;/<span class="hljs-name">script</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">body</span></span></span><span>&gt;
</span><span><span class="hljs-tag">&lt;/<span class="hljs-name">html</span></span></span><span>&gt;
</span></span></code></div></div></pre>
<hr data-start="2667" data-end="2670">
<h2 data-start="2672" data-end="2689">🔄 How to Use:</h2>
<ol data-start="2690" data-end="2898">
<li data-start="2690" data-end="2727">
<p data-start="2693" data-end="2727">Copy the code into a <code data-start="2714" data-end="2721">.html</code> file.</p>
</li>
<li data-start="2728" data-end="2786">
<p data-start="2731" data-end="2786">Host it somewhere (or open directly in mobile browser).</p>
</li>
<li data-start="2787" data-end="2834">
<p data-start="2790" data-end="2834">Allow <strong data-start="2796" data-end="2812">GPS location</strong> access when prompted.</p>
</li>
<li data-start="2835" data-end="2898">
<p data-start="2838" data-end="2898">You’ll see a red dot representing your character on the map.</p>
</li>
</ol>
<hr data-start="2900" data-end="2903">
<h2 data-start="2905" data-end="2946">🗺 Want a Real Map of Rivermoore Park?</h2>
<p data-start="2948" data-end="2963">I can help you:</p>
<ul data-start="2964" data-end="3087">
<li data-start="2964" data-end="3007">
<p data-start="2966" data-end="3007">Get a satellite image of Rivermoore Park.</p>
</li>
<li data-start="3008" data-end="3047">
<p data-start="3010" data-end="3047">Replace the background image with it.</p>
</li>
<li data-start="3048" data-end="3087">
<p data-start="3050" data-end="3087">Add landmarks (clubhouse, pool, etc).</p>
</li>
</ul>
<hr data-start="3089" data-end="3092">
